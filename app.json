{
  "name": "commits.to",
  "description": "Commitment logging and reliability tracking",
  "website": "http://commits.to",
  "repository": "https://github.com/commitsto/commits.to",
  "environments": {
    "staging": {
      "env": {
        "ENV_NAME": "heroku-staging",
        "API_DOMAIN": "commitstew.com",
        "ENVIRONMENT": "review"
      }
    }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ]
}
